# Solution 1:
class Solution:
    def divisorSubstrings(self, num: int, k: int) -> int:
        num1 = str(num)
        res = [num1[i: j] for i in range(len(num1)) for j in range(i + 1, len(num1) + 1) if len(num1[i:j]) == k] 

        divisor = []
        for i in res:
            i = int(i)            
            if i!=0 and num % i == 0:
                divisor.append(i)
        return len(divisor)
