# Solution 1:
class Solution:
    def flipAndInvertImage(self, image: List[List[int]]) -> List[List[int]]:
        
        result = []
        for row in image:
            result.append(list(map(lambda x: 0 if x == 1 else 1, row[::-1])))
            
        return result
# Solution 2:

  class Solution:
    def flipAndInvertImage(self, image: List[List[int]]) -> List[List[int]]:
        for row in image:
            row.reverse()

        for row in image:
            for i in range(len(row)):
                row[i] = 1 - row[i]

        return image
